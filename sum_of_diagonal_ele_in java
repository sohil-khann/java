import java.util.Scanner;
public class array_sum_dia {
	
	

	public static void main(String[] args) {
		int s=0;
		int[][] a=new int[3][3];
		int q=0;
		Scanner sc=new Scanner(System.in);

		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
			a[i][j]=sc.nextInt();
			
				
		}}
		for(int b=0;b<3;b++) {
			for (int c=0;c<3;c++) {
			System.out.print(a[b][c]);
			
		}
			s=s+a[b][q];
			q++;
			
			System.out.println();}
		
		
System.out.print("sum of diagonal ele. is : "+s);

	}
}
